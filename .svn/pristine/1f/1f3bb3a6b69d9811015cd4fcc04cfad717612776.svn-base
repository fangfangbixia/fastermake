  </select>
  
  
  
  <!-- 查询有意向报价的客户（发送过消息） -->
  <select id="queryByOrderIdAndQuoteMessage" resultMap="BaseResultMap">
     select f.* from factory_info f inner JOIN quote_message q 
     on f.factory_id = q.factory_id and order_id = #{orderId,jdbcType=INTEGER}   
     and f.factory_id not in (select factory_id from supplier_quote_info where  
     order_id = #{orderId,jdbcType=INTEGER}) and 
     f.factory_id != #{factoryId,jdbcType=VARCHAR}
     GROUP BY f.factory_id
  </select>
  
  
  <select id="selectStatisticsCount" resultType="map">
      select monthLoginCount,weekLoginCount,totalLoginCount,collectCount,quoteCount,orderCount,inquiryClickCount from factory_info f
        LEFT JOIN
        factory_user u
        on f.factory_id = u.factory_id
        LEFT JOIN
        (select count(1) as monthLoginCount,login_email from login_log where login_email is not null and login_email != '' and login_time > DATE_SUB(CURDATE(), INTERVAL 30 DAY) GROUP BY login_email)a1
        on u.email = a1.login_email
        LEFT JOIN
        (select count(1) as weekLoginCount,login_email from login_log where login_email is not null and login_email != '' and login_time > DATE_SUB(CURDATE(), INTERVAL 7 DAY) GROUP BY login_email)a2
        on u.email = a2.login_email
        LEFT JOIN
        (select count(1) as totalLoginCount,login_email from login_log where login_email is not null and login_email != '' GROUP BY login_email)a3
        on u.email = a3.login_email
        LEFT JOIN
        (select count(1) as collectCount,factory_id from collect_tab GROUP BY factory_id)c
        on f.factory_id = c.factory_id
        LEFT JOIN
        (select count(1) as quoteCount,factory_id from supplier_quote_info GROUP BY factory_id)q
        on f.factory_id = q.factory_id
        LEFT JOIN
        (select count(1) as orderCount,order_factory_id from order_factory_tab GROUP BY order_factory_id)o
        on f.factory_id = o.order_factory_id
        LEFT JOIN
        (select count(1) as inquiryClickCount,factory_id from inquiry_click_log GROUP BY factory_id)i
        on f.factory_id = i.factory_id
        where f.factory_id = #{factoryId,jdbcType=VARCHAR}
  </select>
  
  
  
