    // 使用刚指定的配置项和数据显示图表。



    function echartPic(i,obj){

        var option = {
            title : {
                /*text: '基础雷达图'*/
            },
            tooltip : {
                /*trigger: 'item',
                formatter: "{b}: {c} ({d}%)"*/
            },
            legend : {
                /*data: ['预算分配（Allocated Budget）']*/
            },
            symbolSize : '2',
            radar : {
                splitNumber : 4,
                radius : "50%",
                shape : 'circle',
                name : {
                    textStyle : {
                        color : '#333',
                        /*backgroundColor: '#999',
                        borderRadius: 3,
                        padding: [3, 5]*/
                    }
                },
                indicator : [ {
                    name : '质量',
                    max : 100
                }, {
                    name : '价格',
                    max : 100
                }, {
                    name : '沟通',
                    max : 100
                }, {
                    name : '交期',
                    max : 100
                } ],
                splitArea : {
                    show : true,
                    areaStyle : {
                        type : "default",
                        color : "rgb(255, 255, 255)"
                    }
                }
            },
            series : [ {
                /*name: '预算 vs 开销（Budget vs spending）',*/
                type : 'radar',
                areaStyle : {
                    normal : {
                        color : "rgba(48,159,255,0.4)"
                    }
                },
                data : [ {
                    value : [ 80, 80, 80, 90 ],
                    name : '预算分配'
                } ],
                itemStyle : {
                    normal : {
                        lineStyle : {
                            color : 'rgba(48,159,255,0.4)'
                        }
                    }
                },
                symbolSize : 0
            } ]
        };
        // 使用刚指定的配置项和数据显示图表。


        var quality='';
        var price = '';
        var connect = '';
        var delivery = '';
        if(obj){
            var factoryEchart = obj;
            quality = factoryEchart.quality;
            price = factoryEchart.price;
            connect = factoryEchart.connect;
            delivery = factoryEchart.delivery;
        }
        option.series[0].data[0].value = []
        option.series[0].data[0].value.push(80)
        option.series[0].data[0].value.push(80)
        option.series[0].data[0].value.push(80)
        option.series[0].data[0].value.push(90)

        var myChart = echarts.init(document.getElementById('rating_matrix_'+i));
        myChart.setOption(option);

    }

