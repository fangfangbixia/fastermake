/*area2*/
.div_list{
	width:100%;
	padding:0 15px;
	overflow: hidden;
}
.cont_list{
	width:100%;
	overflow: hidden;
}
.dl_list{
	float: left;
	width:228px;
	height:260px;
	border:1px solid #d1d1d1;
	box-sizing: border-box;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	margin-right: 15px;
	margin-bottom:20px;
}
.dl_list:nth-child(3n){
	margin-right: 0;
}
.dt_list{
	display: table-cell;
	width:230px;
	height:156px;
	border-bottom: 1px solid #d1d1d1;
	vertical-align: middle;
	text-align: center;
}
.dt_list img{
	max-width:100%;
	max-height:100%;
	border:0;
}
.dd_list{
	width:100%;
	overflow: hidden;
	padding:4px 0;
	background-color:#f5f5f5;
}
.p_list{
	width:100%;
	overflow: hidden;
	margin-bottom: 0;
	line-height: 18px;
	
}
.span_list1{
	display:inline-block;
	width:25%;
