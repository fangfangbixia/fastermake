.box-banner{
	width:770px;
	margin:0 auto;
}
.clearfix:after {clear: both;content: '';display: block;font-size: 0;line-height: 0;visibility: hidden;width: 0;height: 0;}
.imgnav {
    position: relative;
    width: 100%;
}
#img {
    z-index: 10;
    width: 100%;
    position: relative;
    text-align: center;
}
#img .div_video{
	display:none;
	height:320px;
}
.div_add{	
	text-align: center;
	width: 380px;
    height: 320px;	
}
.pic_add{
	max-width: 380px;
    height: auto;
    max-height: 320px;
	width: auto;
	display: inline-block;
}

.div_video .video{display: inline-block;width:380px;height:320px;}
#prev{  position:absolute; left:60px; top:110px;display:block; background:url(../images/imgSlideRight.png) no-repeat center center; width:100px; height:100px; }
#next{  position:absolute; right:60px; top:110px; display:block; background:url(../images/imgSlideLeft.png) no-repeat center center; width:100px; height:100px;}

#cbtn{position:relative; height:130px;margin:100px 0; }
#cbtn ul{position:relative; width:10000px; height:130px;}
#cbtn ul li{ float:left; width:130px; height:130px; cursor:pointer; margin-right:10px;box-sizing: border-box;}
#cbtn li.hov{border:2px solid red;box-sizing: border-box;}
#cbtn ul li img{ width:126px; height:126px; display:block;border:0;}
#cSlideUl{width:690px; height:130px;overflow:hidden; position:relative;margin:0 auto;}
.picSildeLeft{cursor:pointer; position:absolute; top:25px; left:0px; z-index:9;}
.picSildeRight{cursor:pointer; position:absolute; top:25px; right:0px; z-index:9;}
/*box-05宸ュ巶鍙ｇ*/
.item_box{width:740px;margin:0 auto 8px;overflow: hidden;position: relative;}
.item_all{display: inline-block;height:30px;line-height: 44px;font-size: 13px;color:#999;padding-left:10px;float:left}
.num_color{color:red;}
.go_evaluate{margin-right:5px;display: block;width:125px;height:30px;line-height: 30px;text-align: center;border-radius: 4px;background:#006DCC;color:#fff;float:right;font-size: 14px;text-decoration: none;}
.go_evaluate:hover{
	color:#fff;
	background-color: #0543be;
}
.area_box{width:100%;overflow: hidden;}
.area_common{width:760px;margin:0 auto 20px;position: relative;overflow: hidden;background: #f2f2f2;}
.readonly_area{width:100%;padding:10px 10px 45px;overflow: hidden;line-height: 20px;color:#666;font-size: 14px;text-align: justify;border-radius: 4px;}
.area_time{position: absolute;left:10px;bottom:10px;display: inline-block;font-size: 12px;color:#999;}
.area_zan{cursor:pointer;position: absolute;right:10px;bottom:10px;display: inline-block;font-size: 12px;}
.icon-zan{font-size: 20px;}
.area_zan:hover{color:#006DCC;}
.area_zan_alreadly{color:#006DCC;}
/*鍘昏瘎璁哄脊妗�*/
.evaluate_box{display:none;position: absolute;left:0;top:0;width:100%;height:100%;background:rgba(0,0,0,0.5);z-index: 99;}
.evaluate_container{position: absolute;left:0;top:0;right: 0;bottom: 0;margin:auto;width:550px;height:400px;background: #fff;border-radius: 6px;padding:20px 15px;}
.evaluate_row{height:auto;}
.evaluate_tit{width:100%;overflow: hidden;line-height: 40px;}
.evaluate_left{float:left;display: inline-block;width:25%;color:#333;font-size: 14px;}
.evaluate_right{float:left;display: inline-block;width:70%;color: #006DCC;font-size: 16px;position: relative;}
.evaluate_txtarea{float:left;display: inline-block;width:70%;color: #006DCC;font-size: 13px;line-height: 16px;border:1px solid #b1b1b1;height: 150px;}
.a_file{border:1px solid #b1b1b1;border-radius: 4px;font-size: 14px;padding:5px 8px;}
.btn-upload{position: relative; display:inline-block;height:36px; *display:inline;overflow:hidden;vertical-align:middle;cursor:pointer}
.upload-url{cursor: pointer}
.input-file{position:absolute; right:0; top:0; cursor: pointer; z-index:1; font-size:30em; *font-size:30px;opacity:0;filter: alpha(opacity=0)}
.btn-upload .input-text{ width:auto}
.form-group .upload-btn{ margin-left:-1px}
.evaluate_right .form-group{margin:15px auto 20px;}
.btn-upload .radius-txt{width: 250px;height: 31px;border: 1px solid #b1b1b1;border-radius: 4px;}
.evaluate_pic{margin-top:15px;}
.star-color{color:red;}
.evaluate_lineheight{line-height: 12px;}
.evaluate_btn{width:100%;height:50px;position: relative;margin-top:20px;text-align: center;}
.evaluate_btn a{display:inline-block;width:100px;height:40px;line-height: 40px;text-align: center;color:#fff;background: #006DCC;border-radius: 4px;}
.evaluate_btn .evaluate_cancel{background: #fff;border: 1px solid #d1d1d1;color:#333;margin-left:20px;}
