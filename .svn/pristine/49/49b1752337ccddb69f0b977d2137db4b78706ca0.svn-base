<script>
    $('#header').load('header.html');
    $('#footer').load('footer.html');
    /*头部样式切换*/
    $('.nav_head a:not(".a1")').click(function () {
        $('.nav_head a').css({
            'text-decotation': 'none'
        });
		/*$(this).css({
			'background' : 'url(/images/products/t4.png)'
		});*/
    })
    /* 日历插件*/
    $('.form_date').datetimepicker({
        language: 'zh-en',
        weekStart: 1,
        todayBtn: 1,
        autoclose: 1,
        todayHighlight: 1,
        startView: 2,
        minView: 4,
        forceParse: 0
    });
</script>
<script>
    $('.quickly_release_en .top input').focus(function () {
        $('.div_transparent').show();
        $(this).parent().siblings('ul').show();
    })
    $('.quickly_release_en .top input').click(function () {
        $('.div_transparent').show();
        $(this).parent().siblings('ul').show();
    })
    $('.quickly_release_en .top input').keydown(function () {
        $('.div_transparent').hide();
        $(this).parent().siblings('ul').hide();
    })
    $('.quickly_release_en .td_input_select ul li').click(function () {
        $('.div_transparent').hide();
        $(this).parent().hide();
        var li_val = $(this).text();
        $(this).parent().siblings('.top').find('input').val(li_val);
    })
    $('.div_transparent').click(function () {
        $('.div_transparent').hide();
        $('.quickly_release_en .td_input_select ul').hide();
    })
    //注意：火狐使用event时， 
    function inputNum(evt) {
        //火狐使用evt.which获取键盘按键值，IE使用window.event.keyCode获取键盘按键值
        var ev = evt.which ? evt.which : window.event.keyCode;
        if (ev >= 48 && ev <= 57) {
            return true;
        } else {
            return false;
        }
    }
    //Add 添加表格 add_tr
    $('.add_tr').click(function () {
        // 		$('#firstProduct tr:last-child input,#firstProduct tr:last-child textarea').val('');
        var tr_length = $('#firstProduct tr').length;
        var name_val = $('#productName').val();
        var qty_val = $('#productQty').val();
        if (name_val == "" || qty_val == "") {
            easyDialog.open({
                container: {
                    content: 'Partname or Qty can not be empty.'
                },
                overlay: false,
                autoClose: 1500
            });
            return false;
        }
        if (tr_length > 9) {
            easyDialog.open({
                container: {
                    content: 'The maximum number of 10'
                },
                overlay: false,
                autoClose: 1500
            });
            return false;
        }
        var addtr = $('#firstProduct tr:first-child').clone(true);
        $('#firstProduct').append(addtr);
        if (tr_length > 0) {
            $('#firstProduct').find('tr').not(':first').find('td:last').find('.z-pro-del').css({ 'visibility': 'visible' });
        }
    });
    function del_pro(obj) {
        $(obj).parents('tr').remove();
    }

</script>