<script>
    $('#header').load('header.html');
    $('#footer').load('footer.html');
    /*头部样式切换*/
    $('.nav_head a:not(".a1")').click(function () {
        $('.nav_head a').css({
            'text-decotation': 'none'
        });
		/*$(this).css({
			'background' : 'url(../images/products/t4.png)'
		});*/
    })
    /*选者框样式切换*/
    $('.checkbox label').click(function () {
        if ($('label em').css('display') == 'block') {
            $('label em').css({
                'display': 'none'
            });
        } else {
            $('label em').css({
                'display': 'block'
            });
        }
    })
    /* 资格认证多选切换*/
    $('.product_invitation .zgrzinp').click(function () {
        $('.product_invitation  .zgrzdx').toggle();
    })
    $('.product_invitation .selspan').click(function () {
        $('.product_invitation  .zgrzdx').toggle();
    })
    /*询盘类别扩大选中区域*/
    $('.product_invitation .inps,.product_invitation .pics').click(function () {
        $('.product_invitation .inps input').attr('checked', 'checked');
    })
    /* 日历插件*/
    $('.form_date').datetimepicker({
        language: 'zh-CN',
        weekStart: 1,
        todayBtn: 1,
        autoclose: 1,
        todayHighlight: 1,
        startView: 2,
        minView: 4,
        forceParse: 0
    });
    /* 询盘信息操作*/
    $('.quoteInfo tbody td:even').css({
        'padding-left': '0',
        'background-color': '#f0f0f0',
        'color': '#666',
        'text-align': 'left',
        'width': '410px',
        'padding-left': '15px'
    })
    $('.factoryInfo tbody td:even').css({
        'width': '160px'
    })
    /*控制零件备注*/
    $('.remarks textarea').focus(function () {
        $(this).css({
            'height': 'auto',
        })
    })
    $('.remarks textarea').blur(function () {
        $(this).css({
            'height': '36px',
            'overflow-y': 'scroll'
        })
    })

    /* 控制目标采购单价样式 */
    $('.remarks input').focus(function () {
        $('.remarks .input-group-addon').css({
            'color': '#006dcc',
            'border-color': '#0871cd',
            'box-shadow': 'inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(102,175,233,.6)'
        })
    })
    $('.remarks input').blur(function () {
        $('.remarks .input-group-addon').css({
            'color': '#999',
            'border-color': '#b1b1b1',
            'box-shadow': 'none'
        })
    })

</script>