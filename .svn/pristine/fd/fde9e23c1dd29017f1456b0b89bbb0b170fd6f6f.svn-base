
@font-face {
  font-family: 'iconfont';
  src: url('font/iconfont.eot');
  src: url('font/iconfont.eot?#iefix') format('embedded-opentype'),
  url('font/iconfont.woff') format('woff'),
  url('font/iconfont.ttf') format('truetype'),
  url('font/iconfont.svg#iconfont') format('svg');
}
.iconfont{
  font-family:"iconfont" !important;
  font-size:16px;font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}

.panel{
    background-color: #f5f5f5;
    /* margin-bottom: 0; */
}
.products .products-nav{
    margin-bottom: 0;
}
.panel-default{
    border-color: #f5f5f5;
    box-shadow: none;
}
.products .products-nav .panel-default{
    background-color: #fff;
}
.panel_footer_filter{    
	height: 50px;
    line-height: 50px;
    border-top: 1px solid #ddd;
    padding: 0 40px;
}
.panel_footer_filter>p{    
	font-size: 14px;
	width: 7%;
    margin: 0;
}
.more_filter{
	width: 93%;
	float: right;
	/* margin-top: -1px; */
}
.more_filter>ul{
	margin-bottom: 0;
	text-align: center;
	/* font-size: 0; */
}
.more_filter>ul>li{
	width: 33.33%;
	float: left;
    /* display: inline-block; */
    /* border: 1px solid #ccc; */
    color: #006dcc;
    position: relative;
}
.company-logo{
    width: 60px;
    height: 60px;
    font-size: 14px;
    color: #ffffff;
    border-radius: 30px;
    line-height: 60px;
    text-align: center;
    vertical-align: middle;
    float: left;
    border: 1px solid #ededed;
    overflow: hidden;
}

.factoryLogo{
	max-width: 58px;
	max-height:58px;
	width:auto;
	height:auto;
}
.company-member-name .hai-title-link{
	font-size: 20px;
    color: #333;
    font-weight: 600;
    padding-top: 16px;
    float: left;
    margin:0 0 0 10px;
}
.memb-background{
	float: left;
	background-color: #ff6f30;
	margin-top: 19px;
	padding-left: 9px;
    padding-right: 18px;
    position: relative;
    margin-left: 20px;
}
.memb-background:after{
	content: "";
	display: block;
	width: 18px;
    height: 18px;
    position: absolute;
    right: -9px;
    top: 3px;
	background-color: #fff;
	-ms-transform:rotate(45deg);
	-moz-transform:rotate(45deg);
	-webkit-transform:rotate(45deg);
	-o-transform:rotate(45deg);
	transform: rotate(45deg);
}
.memb-background h4{
	font-size: 14px;
	color: #FFF;
	display: inline;
    line-height: 24px;
}
.list-cont-body{
    padding-top: 8px;
}
.list-cont-body .dl-horizontal dt,.list-cont-body .dl-horizontal dd{
    height: 34px;
    line-height: 34px;

}
.list-cont-body .dl-horizontal dt{
    max-width: 130px;
    text-align: left;
    padding-left: 30px;
    font-size: 14px;
    color: #999;
    font-weight: 200;
    display: inline-block;
}
.list-cont-body .dl-horizontal dd{
    margin-left: 130px;
    font-size: 14px;
    color: #666;
    overflow: hidden;
    
}
.main-content{
    /* background-color: #fff; */
}
.content-info{
    background-color: #fff;
    margin-bottom: 20px;
}
.default-box{
    float: left;
  	margin-right:20px;
}
.default-box .btn{
    width:120px;
    height: 34px;
    text-align:cetner;
    line-height:34px;
    padding:0;
    color: #666;
}

.optionsTitle{
    border: 2px solid #fff;
    overflow: hidden;
    position: relative;
    z-index: 10005;
    background: #fff;
    height: 48px;
}
.more-condition:hover .optionsTitle {
    border: 2px solid #337ab7;
    border-bottom: none;
    height: 50px;
}

.cut {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.title-cr {
    color: #f89236!important;
    font-weight: bold;
}
.span-text {
    font-size: 14px;
    line-height: 21px;
    vertical-align: baseline;
}
.sonOption {
    position: absolute;
    top: 48px;
    width: 480px;
    border: 2px solid #337ab7;
    z-index: 20001;
    display: none;
}
.sonOptionOl {
    background: #fff;
    width: 476px;
    padding: 6px 16px;
}
.sonOptionOl ol {
    overflow: visible!important;
}
.dp_layout .dp_layout_filter ol {
    height: 22px;
    overflow: hidden;
    padding-left: 0px;
}
.sonOptionOl ol li {
    height: 30px;
    line-height: 30px;
    width: 33%;
    float: left;
    text-align: left;
}
.dp_layout .dp_layout_filter li {
    float: left;
    padding-bottom: 3px;
    margin-right: 6px;
    display: inline;
    position: relative;
}
.sonOptionOl ol li a {
    width: 130px;
    text-decoration: none;
}
.panel_footer_filter .moreProp li a {
    display: inline-block;
    line-height: 20px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    text-decoration: none;
    padding-left: 5px;
    padding-right: 25px;
    color: #666;
    border: 1px solid transparent;
}
.dp_layout .dp_layout_filter li a {
    display: inline-block;
    line-height: 20px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    text-decoration: none;
    padding-left: 5px;
    padding-right: 25px;
    color: #666;
    border: 1px solid transparent;
}
.boxText {
    font-size: 14px;
    vertical-align: middle;
    padding-left: 20px;
    position: relative;
    color: #333;
}
.dp_layout .more_select_btn, .multi_select_btn {
    padding: 1px 0px 15px 0;
    position: absolute;
    text-align: center;
}
.con_inline {
    float: left;
    margin-top: 10px;
    position: inherit!important;
    margin-left: 149px;
}
.multi_select_btn a.btn-sure,a.btn-cancel{
    position: absolute;
    display: inline-block;
    padding: 4px 17px;
    line-height: 20px;
    text-indent: 1px;
    color: #666;
    overflow: hidden;
    border: 1px solid #D1D1D1;
}
.con_inline a {
    float: left;
    position: inherit!important;
}
.con_inline a:nth-child(2) {
    margin-left: 20px;
}
.con_inline a:hover{
    background-color: #006DCC;
    border-color: #006DCC;
}
.con_inline a:hover span{
    color: #fff;
}
.con_inline a span{
    font-size: 14px;
}
.panel_footer_filter .sel-detail span .i0{
    display: block;
    width: 14px;
    height: 14px;
    border: 1px solid #006dcc;
    position: absolute;
    top: 3px;
    left: 0;
    border-radius: 4px;
    display: none;
}
.panel_footer_filter .sel-detail span .iactive{
    display: none;
    width: 14px;
    height: 14px;
    position: absolute;
    top: 3px;
    left: 0;
    border-radius: 4px;
    background: url(../images/products/check.png) no-repeat;
}
.rating-matrix {
    min-width: 150px;
    max-width: 400px;
    height: 200px;
    margin-top: 20px;
}
.highcharts-button {
    display: none;
}
.comments_echart_row_1{
	border-top: 1px solid #ddd;
    padding-top: 15px;
    margin-left: 30px;
    margin-right: 30px;
}
.comments_echart .w275{width:275px;}
.comments_echart .w140{width:140px;}






















